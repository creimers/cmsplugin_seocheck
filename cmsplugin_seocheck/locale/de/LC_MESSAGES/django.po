#: templates/cmsplugin_seocheck/_check_modal.html:12
msgid "Title"
msgstr "Titel"

#: templates/cmsplugin_seocheck/_check_modal.html:13
msgid "The page has a title."
msgstr "Die Seite hat einen Titel."

#: templates/cmsplugin_seocheck/_check_modal.html:15
msgid "The title is"
msgstr "Der Titel ist"

#: templates/cmsplugin_seocheck/_check_modal.html:15
msgid "characters long."
msgstr "Zeichen lang."

#: templates/cmsplugin_seocheck/_check_modal.html:16
msgid "The recommended length is between 40 and 70 characters."
msgstr "Die emfohlene Länge ist 40 bis 70 Zeichen."

#: templates/cmsplugin_seocheck/_check_modal.html:17
msgid "That is a good length."
msgstr "Das ist einge gute Länge."

#: templates/cmsplugin_seocheck/_check_modal.html:19
msgid "The page doesn't have a title yet."
msgstr "Die Seite hat noch keinen Titel."

#: templates/cmsplugin_seocheck/_check_modal.html:21
msgid "Meta description."
msgstr "Meta-Description."

#: templates/cmsplugin_seocheck/_check_modal.html:22
msgid "The page has a meta description."
msgstr "Die Seite hat eine Meta-Description."

#: templates/cmsplugin_seocheck/_check_modal.html:23
msgid "The meta description has a length of"
msgstr "Die Meta-Description hat eine Länge von"

#: templates/cmsplugin_seocheck/_check_modal.html:23
msgid "characters."
msgstr "Zeichen."

#: templates/cmsplugin_seocheck/_check_modal.html:23
msgid "A max of 156 characters is recommended."
msgstr "Die empfohlene Maximallänge ist 156 Zeichen."

#: templates/cmsplugin_seocheck/_check_modal.html:24
msgid "The page doesn't have a meta description."
msgstr "Die Seite hat eine Meta-Description."

#: templates/cmsplugin_seocheck/_check_modal.html:26
msgid "Headlines"
msgstr "Überschriften"

#: templates/cmsplugin_seocheck/_check_modal.html:27
msgid "There is exactly one h1 heading on this page."
msgstr "Auf dieser Seite ist genau eine h1-Überschrift."

#: templates/cmsplugin_seocheck/_check_modal.html:28
msgid "There are multiple h1 headings on this page."
msgstr "Auf dieser Seite sind mehrere h1-Überschriften vorhanden."

#: templates/cmsplugin_seocheck/_check_modal.html:29
msgid "This page doesn't have a h1 title."
msgstr "Die Seite hat keine h1-Überschrift."

#: templates/cmsplugin_seocheck/_check_modal.html:31
msgid "This page has sub headings."
msgstr "Diese Seite hat Unterüberschrieben."

#: templates/cmsplugin_seocheck/_check_modal.html:32
msgid "This page has no sub headings."
msgstr "Diese Seite hat keine Unterüberschriften."

#: templates/cmsplugin_seocheck/_check_modal.html:35
msgid "There is text on this page. It is"
msgstr "Auf der Seite ist Text vorhanden. Dieser ist"

#: templates/cmsplugin_seocheck/_check_modal.html:35
msgid "words long."
msgstr "Wörter lang."

#: templates/cmsplugin_seocheck/_check_modal.html:36
msgid "There is no text on this page."
msgstr "Auf der Seite ist kein Text vorhanden."
